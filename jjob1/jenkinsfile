pipeline {
    agent any
 
    stages {
        stage ("git checkout") {
  	        steps {
	            git branch: 'main', url: 'https://github.com/RaghavendraNaik-cloud/ci-cd-project.git'
            }
        }
    	
  
        stage ("build") {
            steps {
 	            echo "hello build"
	        }
	    }
    }
}